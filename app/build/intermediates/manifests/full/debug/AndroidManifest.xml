<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.brandon.photogalleryapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.brandon.photogalleryapp.GalleryActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.brandon.photogalleryapp.SettingsActivity" />
        <activity android:name="com.brandon.photogalleryapp.DisplayActivity" />
        <activity android:name="com.brandon.photogalleryapp.EditActivity" />
        <activity android:name="com.brandon.photogalleryapp.FilterActivity" />
        <activity android:name="com.brandon.photogalleryapp.TagActivity" />
        <activity android:name="com.brandon.photogalleryapp.CaptionActivity" />
        <activity android:name="com.brandon.photogalleryapp.SearchActivity" />
        <activity android:name="com.brandon.photogalleryapp.SearchedGalleryActivity" />
    </application>

</manifest>